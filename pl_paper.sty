                                                          %%
\newcommand\maketitlepage {                               %%
    \pagenumbering{gobble}                                %%
    \newgeometry{top=2in}                                 %%
    \centering\Large\textbf{\@title}\\                    %%
    \vspace{0.75in}                                       %%
    \@date                                                %%
                                                          %%
    \vspace{\fill}                                        %%
    \small                                                %%
                                                          %%
    \begin{tabularx}{\textwidth}{Y Y Y}                   %%
        \@author                                          %%
    \end{tabularx}                                        %%
    \newgeometry{margin=1in}                              %%
    \justify\normalsize                                   %%
}                                                         %%
