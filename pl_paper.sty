%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
%%              Section Title Formatting                  %%
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
\newcommand\chapter[1]{                                   %%
    \columnbreak                                          %%
    \section{#1}                                          %%
}                                                         %%
                                                          %%
\titleformat{\section}                                    %%
        {\hrule\Large\bfseries}                           %%
        {\smash{\thesection\hspace{1em}}}                 %%
        {0em}                                             %%
        {}                                                %%
        [\hrule]                                          %%
                                                          %%
\titleformat{\subsection}                                 %%
        {\vspace{-0.25cm}\normalsize\bfseries}            %%
        {\smash{\thesubsection\hspace{1em}}}              %%
        {0em}                                             %%
        {}                                                %%
        [\hrule]                                          %%
                                                          %%
\titleformat{\subsubsection}                              %%
        {\vspace{-0.25cm}\normalsize\bfseries}            %%
        {\smash{\thesubsubsection\hspace{1em}}}           %%
        {0em}                                             %%
       {}                                                 %%
